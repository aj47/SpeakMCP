{
  "name": "@speakmcp/mcp-whatsapp",
  "version": "1.0.0",
  "description": "WhatsApp MCP server for SpeakMCP - enables WhatsApp messaging via Model Context Protocol",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "mcp-whatsapp": "./dist/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@whiskeysockets/baileys": "7.0.0-rc.9",
    "pino": "^9.0.0",
    "qrcode-terminal": "^0.12.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "mcp",
    "whatsapp",
    "speakmcp",
    "ai",
    "assistant",
    "baileys"
  ],
  "private": true
}
